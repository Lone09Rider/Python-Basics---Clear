   "source": [
    "class Point:\n",
    "    def __init__(self, x, y):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        \n",
    "    @classmethod\n",
    "    def zero(cls):\n",
    "        return cls(0, 0)\n",
    "        \n",
    "        \n",
    "    def show(self):\n",
    "        print(f\"Point is : ({self.x}, {self.y})\")\n",
    "\n",
    "pt = Point.zero()\n",
    "pt.show()"
   ]
